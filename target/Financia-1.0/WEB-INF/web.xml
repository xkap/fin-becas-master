<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>MantenedorBeneficios</servlet-name>
        <servlet-class>web.Mantenedores.MantenedorBeneficios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MantenedorPorcentajes</servlet-name>
        <servlet-class>web.Mantenedores.MantenedorPorcentajes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MantenedorPorcentajes</servlet-name>
        <url-pattern>/MantenedorPorcentajes</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BuscaAlumno</servlet-name>
        <servlet-class>web.BuscaAlumno</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MantenedorBeneficios</servlet-name>
        <url-pattern>/MantenedorBeneficios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscaAlumno</servlet-name>
        <url-pattern>/BuscaAlumno</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
